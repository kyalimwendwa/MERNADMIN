      .column-left {
       overflow-y: scroll;
       scrollbar-color: #edeeef;
       scrollbar-width: thin;
      /* Adjust the height as needed */
       padding-right: 1rem;
   }
   
   .column-left::-webkit-scrollbar {
       width: 12px;
       height: 2px;
   }
   
   .column-left::-webkit-scrollbar-thumb {
     background-color: #edeeef;
       border-radius: 0;
       border: none;
       outline: none;
       height: 25%;
   }
   
   .column-left::-webkit-scrollbar-track {
       background-color: #fff;
   }
   
       .tilted-svg {
         transform: rotate(-45deg);
       }
       #fileInput {
               display: none;
           }
   
           /* Style for the custom-styled button or label */
           #customButton {
               background-color: #fff;
               color: #213047;
               padding: 10px 15px;
               cursor: pointer;
               border-radius: 5px;
               display: inline-block;
           }
   
           /* Style for hover effect */
           #customButton:hover {
               color: #457cdb;
           }
   
           .arrow-up,
  .arrow-down {
    cursor: pointer;
  }
      /* styles.css */

/* styles.css */

.pagination-container {
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .pagination {
    display: flex;
    list-style: none;
    padding: 0;
  }

  .pagination button {
    width: 40px;
    height: 40px;
    border: 1px solid #acbebe;
    background-color: #fff;
    cursor: pointer;
    border-radius: 5px;
    margin: 0 5px;
  }

  .pagination button.active {
    background-color: #2596be;
    color: #fff;
    border-color: #2596be;
  }
  .pagination-container .button{
    border: 1px solid #fff;
    width: 40px;
    height: 40px;
    border-radius: 5px;
    background-color: #fff;
  }
  .pagination-container .button:hover{
    background-color: #457cdb;
    cursor: pointer;
  }
 
    </style>
   
