    <div class="container" style="display: block; border: 1px solid #acbebe; border-radius: 8px; width: max-content;">

      <div class="flex" style="display: flex; padding: 2rem;">
        <input type="text" id="searchInput" placeholder="Search by name..."  style="display: flex;
        width: 200px; border-bottom: 1px solid #acbebe; outline: none; border-top: none;
        border-right: none;
        border-left: none; height: 40px;
        padding: 1rem;">
      </div>
        <!-- Add search box -->

        <table style="margin-top: 1rem; ">
            <thead style="height: 50px; border-left: none; border-right: none; border-top: 1px solid #acbebe;
            border-bottom: 1px solid #acbebe; width: 100%;
            background-color: #f8fafd; white-space: nowrap; overflow: hidden; text-overflow: ellipsis; text-align: center; ">
              <tr class="tr1">
                <th><input type="checkbox" style="border: 1px solid #acbebe; width: 13px; height: 13px; border-radius: 5px; outline: none;
                  background-color: #fff;
                  "></th>
                <th>IMAGE</th>
                <th style="width: 150px;">NAME</th>
                <th>CATEGORY</th>
                <th>STOCK</th>
                <th>PRICE</th>
                
                <th>ACTIONS</th>
            </tr>
            </thead>
            <tbody id="customerTableBody">
              <% products.forEach(product => { %>
                <tr class="tr2">
                  <td><input type="checkbox" style="border: 1px solid #acbebe; width: 13px; height: 13px; border-radius: 5px; outline: none;
                    background-color: #fff;
                    "></td>
                    <td><img src="data:<%= product.mainImage.contentType %>;base64,<%= product.mainImage.data.toString('base64') %>" alt="Product Image"></td>


                    <td style="width: 100px;"><%= product.name %></td>

                    <td><%= product.category %></td>
                    <td><%= product.stock %></td>
                    <td><%= product.price %></td>
                    
                    <td>
                        <div class="flex" style="display: flex;  justify-content: center; align-items: center;">
                            <div onclick="redirectToOrderDetails('<%= product.name %>')" class="view" style="width: 80px; height: 35px; border: 1px solid #acbebe; display: flex; justify-content: center; align-items: center; border-bottom-left-radius: 5px; border-top-left-radius: 5px;">
                                <svg class="path" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="18" height="18" fill="#acbebe"><path d="M12.0003 3C17.3924 3 21.8784 6.87976 22.8189 12C21.8784 17.1202 17.3924 21 12.0003 21C6.60812 21 2.12215 17.1202 1.18164 12C2.12215 6.87976 6.60812 3 12.0003 3ZM12.0003 19C16.2359 19 19.8603 16.052 20.7777 12C19.8603 7.94803 16.2359 5 12.0003 5C7.7646 5 4.14022 7.94803 3.22278 12C4.14022 16.052 7.7646 19 12.0003 19ZM12.0003 16.5C9.51498 16.5 7.50026 14.4853 7.50026 12C7.50026 9.51472 9.51498 7.5 12.0003 7.5C14.4855 7.5 16.5003 9.51472 16.5003 12C16.5003 14.4853 14.4855 16.5 12.0003 16.5ZM12.0003 14.5C13.381 14.5 14.5003 13.3807 14.5003 12C14.5003 10.6193 13.381 9.5 12.0003 9.5C10.6196 9.5 9.50026 10.6193 9.50026 12C9.50026 13.3807 10.6196 14.5 12.0003 14.5Z"></path></svg>
                            </div>
                            <div class="arrow" style="width: 30px; height: 35px; border: 1px solid #acbebe; display: flex; justify-content: center; align-items: center;border-left: none; border-top-right-radius: 5px; border-bottom-right-radius: 5px;">
                                <svg class="path" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="18" height="18" fill="#acbebe"><path d="M11.9999 13.1714L16.9497 8.22168L18.3639 9.63589L11.9999 15.9999L5.63599 9.63589L7.0502 8.22168L11.9999 13.1714Z"></path></svg>
                            </div>
                        </div>
                    </td>
                </tr>
            <% }); %>
